<MangaProject>
  <random_seed>354745</random_seed>
  <resources>
    <aboveground>
      <type>SimpleAsymmetricZOI</type>
      <x_resolution>100</x_resolution>
      <y_resolution>100</y_resolution>
      <domain>
        <x_1>0</x_1>
        <x_2>22</x_2>
        <y_1>0</y_1>
        <y_2>22</y_2>
      </domain>
    </aboveground>
    <belowground>
      <type>FixedSalinity</type>
      <min_x>0</min_x>
      <max_x>22</max_x>
      <salinity>0 0</salinity>
    </belowground>
  </resources>
  <plant_dynamics>
    <type>Bettina</type>
    <mortality>NoGrowth</mortality>
  </plant_dynamics>
  <population>
    <group>
      <name>Avicennia</name>
      <species>Avicennia</species>
      <distribution>
        <type>Random</type>
        <n_individuals>10</n_individuals>
        <n_recruitment_per_step>1</n_recruitment_per_step>
        <domain>
          <x_1>0</x_1>
          <x_2>22</x_2>
          <y_1>0</y_1>
          <y_2>22</y_2>
        </domain>
      </distribution>
    </group>
  </population>
  <time_loop>
    <type>Simple</type>
    <t_start>0</t_start>
    <t_end>5e8</t_end>
    <delta_t>1e6</delta_t>
  </time_loop>
  <output>
    <type>OneFile</type>
    <output_each_nth_timestep>1</output_each_nth_timestep>
    <output_times>[5e8]</output_times>
    <allow_previous_output>True</allow_previous_output>
    <output_dir>Benchmarks/TestOutputs/</output_dir>
  </output>
  <visualization>
    <type>NONE </type>
  </visualization>
</MangaProject>
